{
  "name": "itau-uy-utils",
  "version": "0.6.0",
  "description": "Provides useful utilities for ItaÃº UY website",
  "author": "Avi Tretiak",
  "license": "MIT",
  "private": true,
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "bun run rollup -wc",
    "clean": "del-cli dist",
    "lint": "eslint --ext .ts . && prettier --ignore-path .eslintignore --check .",
    "lint:fix": "eslint --ext .ts --fix . && prettier --ignore-path .eslintignore --write .",
    "ci": "bun run lint",
    "build:js": "bun run rollup -c",
    "build": "cross-env NODE_ENV=production bun run ci && bun run clean && bun run build:js"
  },
  "devDependencies": {
    "@gera2ld/plaid": "~2.7.0",
    "@gera2ld/plaid-rollup": "~2.7.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@violentmonkey/types": "^0.1.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-userscript": "^0.3.2",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "src/**": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}